{
	"boot-source": {
		"kernel_image_path": "share/vmlinux",
		"boot_args": "console=ttyS0 reboot=k panic=1 pci=off loglevel=8 root=/dev/vda ip=172.42.0.2::172.42.0.1:255.255.255.0::eth0:off"
	},
	"drives": [
		{
			"drive_id": "rootfs",
			"path_on_host": "share/rootfs.img",
			"is_root_device": true,
			"is_read_only": false
		}
	],
	"network-interfaces": [
	{
		"iface_id": "eth0",
		"guest_mac": "AA:FC:00:00:00:01",
		"host_dev_name": "tapTestFc"
	}
	],
	"crypto": {
		"crypto_dev_id": "vaccel0",
		"host_crypto_dev": "/dev/vaccel0"
	},
	"machine-config": {
		"vcpu_count": 2,
		"mem_size_mib": 4096,
		"ht_enabled": false
	}
}
